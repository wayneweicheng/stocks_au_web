# SQL Server connection
sql_server_host=
sql_server_port=1433
sql_server_database=
sql_server_user=
sql_server_password=

# Frontend origin(s) for CORS, comma-separated
allowed_origins=http://localhost:3100

# Authentication credentials
ADMIN_USERNAME=admin
ADMIN_PASSWORD=password123

# Charts base directory (local path to prefix ChartPath when serving via /charts)
# Examples:
#   CHART_BASE_DIR=C:\\pattern_charts\\
#   CHART_BASE_DIR=\\\\fileserver\\charts\\
# Backward compatible key names: CHART_BASE_DIR or CHART_BASE_URL
CHART_BASE_DIR=

# IB Gateway automation
# Path to IB Gateway executable (e.g., C:\IB\IBGateway\1013\ibgateway.exe)
IBG_EXE_PATH=
# Credentials used to auto-fill login form
IBG_USERNAME=
IBG_PASSWORD=
# Seconds to wait after killing existing IBG instance before restart
IBG_WAIT_AFTER_KILL_SECONDS=10

# IB API status probe (host/port). If set, status checks this exact endpoint.
# Example: IBG_API_HOST=192.168.20.102
#          IBG_API_PORT=7496
IBG_API_HOST=
IBG_API_PORT=

# IB Gateway UI preferences
# IBG_TRADING_MODE=Live or Paper
IBG_TRADING_MODE=Live

# Optional calibration (relative window coords 0..1). If set, automation clicks these exact points.
# Example: 0.52 means 52% of window width from left; 0.45 means 45% of height from top.
IBG_USERNAME_X_PCT=
IBG_USERNAME_Y_PCT=
IBG_PASSWORD_X_PCT=
IBG_PASSWORD_Y_PCT=